<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mx.edu.utng.lajosefa.fragments.ContactUsFragment">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="mx.edu.utng.lajosefa.activities.MapsActivity" >
        <Spinner
            android:id="@+id/spn_map_type"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/size_53"
            android:background="@color/color_map"
            android:animateLayoutChanges="true"
            ></Spinner>
    </fragment>

</android.support.constraint.ConstraintLayout>
